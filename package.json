		"cors": "^2.8.5",
		"express": "^4.17.1",
		"express-session": "^1.17.0",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.27.0",
		"mongoose": "^5.9.4",
		"mongoose-seed": "^0.6.0",
		"multer": "^1.4.2",
		"passport": "^0.4.1",
