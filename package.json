{
	"name": "chat-app",
	"version": "1.0.0",
	"description": "MERN Chat App",
	"main": "server.js",
	"scripts": {
		"dev": "env $(cat .env | grep -v \"#\" | xargs) NODE_ENV=development webpack serve",
		"prod": "env $(cat .env | grep -v \"#\" | xargs) NODE_ENV=production webpack",
		"serve": "env $(cat .env | grep -v \"#\" | xargs) nodemon --exec babel-node server.js",
		"seed": "env $(cat .env | grep -v \"#\" | xargs) babel-node ./seeds/seeder"
	},
	"keywords": [
		"chat",
		"react",
		"mern",
		"websockets"
	],
	"author": "",
	"license": "ISC",
